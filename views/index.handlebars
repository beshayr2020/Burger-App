<!-- Placeholder -->
<div class="row">
<div id="UneatenBurgers" class="col-4 offset-1">    
    <h1>Uneaten Burgers</h1>
    {{#each unEatenBurgers}}
    <div class="row align-items-center burgerContainer mb-1">
        <div class="col burgerName">{{id}}. {{burger_name}}</div>
        <button class="col-3 align-self-end btn btn-success btnDevour" data-id="{{id}}">Devour It!</button>
    </div>
    {{/each}}    
</div>
<div id="EatenBurgers" class="col-4 offset-2">
    <h1>Eaten Burgers</h1>
    {{#each eatenBurgers}}
    <div class="eatenContainer mb-1">{{id}}. {{burger_name}}</div>
    {{/each}}
</div>
</div>
